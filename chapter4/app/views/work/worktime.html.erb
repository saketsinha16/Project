<h1>work time</h1>

<div>
	<%= form_for :work, url: work_index_url do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
	<br/>
	<%= link_to "Click to go to About Page", :action => "about" %><br/>
	<%= link_to "Click to go to Contact Page", :action => "contact" %>
	
</div>
